<Payload xmlns="http://javaops.ru"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://javaops.ru payload.xsd">
    <Users>
        <User flag="active" city="kiv" email="gmail@gmail.com" id="fn">
            <fullName>Full Name</fullName>
        </User>
        <User flag="superuser" city="spb" email="admin@javaops.ru" id="admin">
            <fullName>Admin</fullName>
        </User>
        <User flag="deleted" city="spb" email="mail@yandex.ru" id="deleted">
            <fullName>Deleted</fullName>
        </User>
    </Users>
    <Cities>
        <City id="spb">Санкт-Петербург</City>
        <City id="kiv">Киев</City>
        <City id="mnsk">Минск</City>
    </Cities>

    <Projects>
        <Project name="topjava" description="this is topjava">
            <Groups>
                <Group type="REGISTERING" fullName="topjava01">
                    <Users>
                        <GroupUser user="fn"></GroupUser>
                    </Users>
                </Group>
                <Group type="REGISTERING" fullName="topjava02">
                    <Users>
                        <GroupUser user="admin"></GroupUser>
                    </Users>
                </Group>
                <Group type="REGISTERING" fullName="topjava03">
                    <Users>
                        <GroupUser user="admin"></GroupUser>
                    </Users>
                </Group>
            </Groups>
        </Project>
        <Project name="masterjava" description="this is masterjava">
            <Groups>
                <Group type="REGISTERING" fullName="masterjava01">
                    <Users>
                        <GroupUser user="admin"></GroupUser>
                        <GroupUser user="deleted"></GroupUser>
                    </Users>
                </Group>
                <Group type="REGISTERING" fullName="masterjava02">
                    <Users>
                        <GroupUser user="admin"></GroupUser>
                        <GroupUser user="deleted"></GroupUser>
                    </Users>
                </Group>
                <Group type="REGISTERING" fullName="masterjava03">
                    <Users>
                        <GroupUser user="deleted"></GroupUser>
                    </Users>
                </Group>
            </Groups>
        </Project>
    </Projects>
</Payload>